{
  "name": "picmo",
  "private": true,
  "version": "6.0.0",
  "type": "module",
  "description": "JavaScript emoji picker. Any app, any framework.",
  "keywords": [
    "emoji",
    "emoji picker",
    "javascript",
    "typescript",
    "svelte"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "lint": "eslint src",
    "format": "prettier --plugin-search-dir=. --write --ignore-unknown src",
    "prepare": "husky install",
    "e2e": "cypress run --component"
  },
  "lint-staged": {
    "src/**/*.{ts,js,svelte}": [
      "eslint src",
      "prettier --plugin-search-dir=. --write --ignore-unknown src"
    ]
  },
  "author": "Joe Attardi <jattardi@gmail.com> (https://joeattardi.dev)",
  "repository": "joeattardi/picmo",
  "homepage": "https://picmojs.com",
  "bugs": "https://github.com/joeattardi/picmo/issues",
  "funding": "https://github.com/sponsors/joeattardi",
  "license": "MIT",
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.1.0",
    "@tsconfig/svelte": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "cypress": "^12.2.0",
    "eslint": "^8.28.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.4",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "svelte": "^3.52.0",
    "svelte-check": "^2.9.2",
    "svelte-preprocess": "^4.10.7",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4",
    "vite": "^3.2.3"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.1.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "emojibase": "^6.1.0",
    "svelte-awesome": "^3.0.0"
  }
}
